# vim:ft=sshconfig ts=2 sw=2 

Include ~/.ssh/local.config

Host home
    StrictHostKeyChecking no
    HostName home.xvzc.dev
    User jerry
    IdentitiesOnly yes
    IdentityFile ~/.ssh/home.pub

Host xvzc.github.com
	HostName github.com
    ForwardAgent yes
    IdentitiesOnly yes
    IdentityFile ~/.ssh/xvzc.pub

